{
  "name": "sanity-plugin-yaml-document",
  "version": "1.0.0",
  "description": "Use YAML to quickly update a Sanity document",
  "main": "lib/Plugin.js",
  "scripts": {
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "prepublishOnly": "sanipack build && sanipack verify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aratramba/sanity-plugin-yaml-document.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Arjen Scherff-de Water <aratramba@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-sanity": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "prettier": "^2.3.1",
    "sanipack": "2.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/aratramba/sanity-plugin-yaml-document/issues"
  },
  "homepage": "https://github.com/aratramba/sanity-plugin-yaml-document#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier",
      "prettier/react"
    ]
  },
  "dependencies": {
    "@sanity/ui": "^0.34.2",
    "@types/js-yaml": "^4.0.1",
    "@types/react": "^17.0.11",
    "codemirror": "^5.61.1",
    "js-yaml": "^4.1.0",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  }
